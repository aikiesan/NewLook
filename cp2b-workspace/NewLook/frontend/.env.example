# CP2B Maps V3 Frontend - Environment Variables Template
# Copy this file to .env.local and fill in the values

# Backend API URL
# Local: http://localhost:8000
# Production: https://newlook-production.up.railway.app
NEXT_PUBLIC_API_URL=http://localhost:8000

# Force Mock Data
# true = Use client-side mock data (bypasses backend API completely)
# false = Use real backend API
# Default: true (if undefined)
NEXT_PUBLIC_USE_MOCK_DATA=true

# Supabase Configuration
# Get these values from: https://supabase.com/dashboard/project/zyuxkzfhkueeipokyhgw/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://zyuxkzfhkueeipokyhgw.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here
